<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/reader_nav_button_ripple">
    <item android:id="@android:id/mask">
        <shape android:shape="rectangle">
            <corners android:radius="24dp" />
            <solid android:color="@color/reader_nav_button_ripple" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="24dp" />
            <solid android:color="@color/reader_nav_button_fill" />
            <stroke
                android:width="1dp"
                android:color="@color/reader_letter_highlight" />
        </shape>
    </item>
</ripple>
